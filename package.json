  "main": "index.js",
  "scripts": {
    "server": "node api/server.js",
    "start": "set NODE_ENV=development & webpack-dev-server --config webpack.dev.js --open",
    "start_prod": "set NODE_ENV=production & webpack serve --config webpack.prod.js --open",
    "build": "set NODE_ENV=production & webpack --config webpack.prod.js",
    "start_all": "concurrently \"npm run server\" \"npm run start\"",
    "start_all_p": "concurrently \"npm run server\" \"npm run start_prod\"",
    "build_all": "concurrently \"npm run server\" \"npm run build\"",
    "rem_dist": "rd /s /q dist",
    "rem_dist_n_do_build": "rd /s /q dist & npm run build",
    "rem_n_ins_nm": "rd /s /q node_modules & del /f package-lock.json & npm install",
    "verify_cache": "npm cache verify --force",
    "clear_cache": "npm cache clear --force",
    "all_cache": "npm run verify_cache && npm run clear_cache && rd /s /q node_modules || del /f /q package-lock.json",
    "myinstall": "npm run all_cache && npm install",
    "help": "scripts/help_cmds.bat",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
